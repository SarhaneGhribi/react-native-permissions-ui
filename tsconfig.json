{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "jsx": "react",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowSyntheticDefaultImports": true,
      "noEmit": true,
      "lib": ["es2015", "dom"]
    },
    "include": ["src", "example"]
  }
  
  